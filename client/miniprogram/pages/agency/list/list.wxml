


<view class="wraps" hover-class="none" hover-stop-propagation="false">
    <block wx:for="{{sell_list}}" wx:key="item">
        <view class="item_wrap" hover-class="none" hover-stop-propagation="false" bindtap="bind_nav_detail"
            data-key="{{item.key}}">
            <image class="image" src="{{item.images}}" mode="aspectFill" lazy-load="false" binderror="" bindload="">

            </image>
            <view class="etb" hover-class="none" hover-stop-propagation="false">
                {{item.name}}
            </view>
            <view class="text" hover-class="none" hover-stop-propagation="false" >
                {{item.sub_name}}
            </view>
            <view class="price" hover-class="none" hover-stop-propagation="false">
                ï¿¥{{item.price}}
            </view>
        </view>
    </block>
</view>