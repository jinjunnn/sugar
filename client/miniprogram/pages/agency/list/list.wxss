/* pages/landing/sell/sell.wxss */
page {
    background-color: rgb(245, 245, 245);
}

.wraps {
    margin: 10rpx;
    display: -webkit-flex;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.item_wrap {
    width: 234rpx;
    margin: 10rpx 5rpx 10rpx 5rpx;
    background-color: white;
}

.image {
    width: 234rpx;
    height: 234rpx;
}

.text {
    width: 234rpx;
    min-height: 61rpx;
    font-size: 22rpx;
    padding: 5rpx;
    color: #3f3f3f;
    /* 多行文字溢出 */
    overflow: hidden;
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.etb {
    width: 234rpx;
    font-size: 22rpx;
    padding: 5rpx;
    color: #3f3f3f;
    /* 多行文字溢出 */
    overflow: hidden;
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.price {
    width: 234rpx;
    font-size: 22rpx;
    padding: 5rpx;
    color: #0b0b0b;
    font-weight: bold;
    /* 多行文字溢出 */
    overflow: hidden;
    display: -webkit-box;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}