.commodity_screen {
    background: #f36062;
    color: #fff;
    width: 550rpx;
    height: 700prx;
    border-radius: 20rpx;
    opacity: 1;
    overflow: hidden;
    z-index: 100100;
    position: fixed;
    top: 10vh;
    left: 0;
    margin: 0rpx 100rpx 0rpx 100rpx;
}


.bg_color {
    background-color: rgba(0, 0, 0, 0.3);
    z-index: 1000;
    width: 100%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 0;
    height: 10000rpx;
}

